{
  "name": "react-native-iap",
  "version": "4.3.2",
  "description": "React Native In App Purchase Module.",
  "main": "index.js",
  "types": "index.d.ts",
  "postinstall": "dooboolab-welcome postinstall",
  "scripts": {
    "build": "tsc && flowgen index.d.ts -o index.js.flow",
    "flow": "flow",
    "lint": "eslint -c .eslintrc.js 'index.ts'",
    "test": "exit 0",
    "test:flow": "flow",
    "test:typescript": "tsc --noEmit",
    "tsc:sync": "npx typesync",
    "tsc": "tsc",
    "validate": "npm run lint && npm run test:typescript && npm run test:flow && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dooboolab/react-native-iap"
  },
  "keywords": [
    "react-native",
    "in app purchase",
    "in-app-purchase",
    "billing"
  ],
  "author": "dooboolab",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.54"
  },
  "dependencies": {
    "dooboolab-welcome": "^1.1.1"
  },
  "devDependencies": {
    "@dooboo/eslint-config": "^0.3.0",
    "@types/eslint": "^6.1.3",
    "@types/react": "16.9.16",
    "@types/react-native": "0.60.24",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "flow-bin": "^0.113.0",
    "flowgen": "^1.10.0",
    "typescript": "^3.6.3"
  }
}
